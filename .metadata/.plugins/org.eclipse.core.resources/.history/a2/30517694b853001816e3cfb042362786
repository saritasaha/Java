package default;

public class ValueOfX {
	
	public static void main(String[] args) {
		int x=1;
		System.out.println("Before the Loop x is" + x);
		while (x > 12) {
			x = x - 1;
		System.out.println("inside the loop");
			}
		for (x=0; x < 10; x = x + 1) {
			System.out.println("x is now" + x);
		
		}
		
	}
}
